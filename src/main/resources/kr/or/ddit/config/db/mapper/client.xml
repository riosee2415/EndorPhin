<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="client">
	
	<!-- 거래처 전체조회 -->
	<select id="getAllClient" resultType="clientVo">
        SELECT
            CLIENTCODE, 
            CLIENTNAME, 
            SALESNUMBER, 
            MANAGER, 
            TELEPHONE, 
            FAXNUMBER, 
            MANAGEREMAIL, 
            BUSINESS, 
            PLACE, 
            RELATE, 
            BUSINESSTYPE, 
            BANKNAME, 
            ACCOUNTNUMBER, 
            USESTATUS
        FROM CLIENT
	</select>

</mapper>



